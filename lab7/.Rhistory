glimpse(penguins)
levels(penguins$island) #used levels because island its a factor as seen in glimspe
penguins %>%
group_by(island) %>%
summarize(min_weight=min(body_mass_g),
max_weight=max(body_mass_g),
mean_weight=mean(body_mass_g),
total=n())
penguins %>%
group_by(island) %>% #group by only works with categorical variable
summarize(number_NAs = sum(is.na(body_mass_g))) ##the one NA values screw up the calculations
penguins %>%
filter(!is.na(body_mass_g)) %>%  #Pull out all of the observations with a number, leave others w/ NA behind
group_by(island) %>%
summarize(mean_weight=mean(body_mass_g))
penguins %>%
group_by(species, island) %>%
summarize(n=n(), .groups= 'keep')#the .groups argument here just prevents a warning message
penguins %>%
count(island, sort = T) #sort=T sorts the column in descending order
penguins %>%
group_by(island) %>%
summarize(n=n())
penguins %>%
count(island, species, sort = T) # sort=T will arrange in descending order
penguins %>%
tabyl(island, species)
penguins %>%
group_by(species) %>%
summarize(mean_bill_length=mean(bill_length_mm, na.rm = T)) #na.rm is inside, if outside it would create a new variable
penguins %>%
count(sex, island)
penguins %>%
summarize(distinct_species = n_distinct(species),
distinct_island = n_distinct(island),
distinct_sex = n_distinct(sex))
penguins %>%
summarize(across(c(species, island, sex), n_distinct))
penguins %>%
summarize(across(contains("mm"), mean, na.rm=T))
penguins %>%
summarize(across(contains("mm"), \(x) mean(x, na.rm = TRUE))) #use this to correct the error above
penguins %>%
group_by(sex) %>%
summarize(across(contains("mm"), mean, na.rm=T))
penguins %>%
summarise_all(mean, na.rm=T)
penguins %>%
summarise(across(!c(species, island, sex, year),
mean, na.rm=T))
penguins %>%
summarise(across(starts_with("bill"), mean, na.rm=T))
